# iOS Automation Environment Configuration

# WebDriverAgent URL (default is localhost with USB tunnel)
WDA_URL=http://localhost:8100

# OCR Configuration
OCR_ENGINE=easyocr
OCR_LANGUAGES=en

# Screenshot save directory
SCREENSHOTS_DIR=./screenshots

# Logging level (DEBUG, INFO, WARNING, ERROR)
LOG_LEVEL=INFO

# Default timeouts (seconds)
DEFAULT_TIMEOUT=10
ACTION_DELAY=0.3
